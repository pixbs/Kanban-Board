{
  "name": "kanban-board",
  "version": "1.0.0",
  "description": "Your Figma widget",
  "scripts": {
    "build": "npm run build-ui && npm run build-widget && node inline-script.js",
    "build-ui": "esbuild iframe-src/index.tsx --bundle --outfile=dist/ui.js --target=es6",
    "build-widget": "esbuild widget-src/index.tsx --bundle --outfile=dist/code.js --target=es6",
    "watch-ui": "npm run build-ui -- --watch",
    "watch-widget": "npm run build-widget -- --watch",
    "watch-inline": "node inline-script.js --watch",
    "tsc": "tsc --noEmit -p widget-src"
  },
  "author": "",
  "license": "",
  "devDependencies": {

  },
  "dependencies": {
    "@figma/plugin-typings": "^1.74.0",
    "@figma/widget-typings": "^1.8.0",
    "esbuild": "^0.19.2",
    "typescript": "*",
    "@esbuild-plugins/node-resolve": "^0.2.2",
    "@types/react-dom": "^18.2.7",
    "csstype": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
